{
  "manifest_version": 2,
  "name": "genericExtension",
  "description": "Awesome Chrome Extension",
  "version": "0.0.1",
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": ["lib/angular/angular.min.js"]
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  }
}